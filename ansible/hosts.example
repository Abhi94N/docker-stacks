all:
  hosts:
    illumidesk:
      ## Required settings
      #-------------------
      # IPv4 host address
      ansible_host: 127.0.0.1

      # IPv4 host address
      ansible_port: 22

      # IPv4 host address
      ansible_user: root

      # IPv4 host address
      ansible_ssh_private_key_file: /path/to/my/key/file
      
      # Explicitly set the python interpreter to python3
      ansible_python_interpreter: /usr/bin/python3
